page {
    background-color: #f3f3f3;
}

.tip {
    padding-top: 30rpx;
    font-size: 32rpx;
    text-align: center;
    color: #40425d;
}

.tipItem {
    padding-top: 30rpx;
    font-size: 32rpx;
    text-align: center;
    color: #40425d;
    text-decoration: underline;
}

.list .li {
    background-color: #fff;
    margin-top: 13rpx;
}

.list .li:nth-child(1) {
    margin-top: 0;
}

.list .li .liWrapper {
    font-size: 26rpx;
    position: relative;
    line-height: 80rpx;
    height: 80rpx;
    border-top: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
}

.li .timeWrapper {
    height: 100%;
    background: center left 30rpx /38rpx auto url("https://www.korjo.cn/xcx/golfImg/i-note-b.png")  no-repeat;
}

.li .timeWrapper .text {
    margin-left: 70rpx;
}

.li .status {
    padding-left: 20rpx;
    line-height: 45rpx;
    height: 45rpx;
    position: absolute;
    top: 50%;
    right: 30rpx;
    color: #12898a;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    border-left: 1px solid #999;
}

.li .content {
    padding: 10rpx 30rpx;
}

.li .content .item{
    line-height: 1.5;
    color: #40425d;
    font-size: 28rpx;
}

.li .hint {
    color: #12898a;
    text-align: center;
    font-size: 25rpx;
    padding-bottom: 10rpx;
}

.li .code {
    height: 85rpx;
    line-height: 85rpx;
    color: #40425d;
    font-size: 32rpx;
    text-align: center;
    background-color: #ecf5fb;
}

.li .cancelWrapper {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    -webkit-align-items: center;
    padding-left: 30rpx;
    position: relative;
    height: 95rpx;
    font-size: 24rpx;
    color: #12898a;
}

.li .cancelWrapper .button {
    color: #12898a;
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    right: 30rpx;
    text-align: center;
    font-size: 25rpx;
    line-height: 1;
    padding: 15rpx 0;
    width: 150rpx;
    border: 1px solid #12898a;
    background-color: #fff;
    border-radius: 0;
}

.li .cancelWrapper .button.cancelled {
    border-color: #dadada;
    color: #626262;
}

.box {
    padding: 70rpx 0 50rpx;
    line-height: 1.8;
    text-align: center;
    background-color: #fff;
    width: 77%;
    margin: 20% auto 5%;
    border-radius: 10rpx;
}

.box .icon {
    width: 28%;
    margin: 0 auto;
}

.box .success {
    color: #12898a;
    font-size: 38rpx;
}

.box .remains {
    color: #40425d;
    font-size: 32rpx;
}

.box .remains text{
    color: #12898a;
}

.box .more {
    color: #12898a;
    font-size: 26rpx;
}

.box .button {
    color: #40425d;
    font-size: 32rpx;
    line-height: 1;
    padding: 20rpx 0;
    width: 32%;
    border: 1px solid #aaa;
    margin: 50rpx auto 0;
    background-color: #fff;
}

.dialogue {
	padding: 70rpx 5%;
}

.dialogue .dialogue_box {
	background-color: #fff;
	border-radius: 15rpx;
}

.dialogue .dialogue_box .boxTitle {
	letter-spacing: 3px;
	position: relative;
	height: 105rpx;
	line-height: 105rpx;
	font-size: 37rpx;
	text-align: center;
	color: #fff;
	background-color: #12898a;
	border-top-left-radius: 15rpx;
	border-top-right-radius: 15rpx;
}

.dialogue .dialogue_box .boxTitle .remove {
	width: 45rpx;
	position: absolute;
	right: 30rpx;
	top: 50%;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
}

.dialogue .dialogue_box .courseDate {
	line-height: 1;
	padding: 35rpx 0;
	text-align: center;
	font-size: 37rpx;
	color: #40425d;
	background: #ecf5f8;
}

.dialogue .dialogue_box .content{
	margin-left: 13%;
	color: #40425d;
	font-size: 35rpx;
}

.dialogue .dialogue_box .content .item {
	margin-top: 35rpx;
}

.dialogue .dialogue_box .buttons {
	padding: 120rpx 11% 70rpx;
}

.dialogue .dialogue_box .buttons .btn  {
	text-align: center;
	border: 1px solid #12898a;
	line-height: 1;
	padding: 20rpx 0;
	width: 48%;
	border-radius: 15rpx;
    font-size: 35rpx;
}

.dialogue .dialogue_box .buttons .cancel {
	float: left;
    background-color: #12898a;
	color: #fff;
}

.dialogue .dialogue_box .buttons form .conform {
	float: right;
	color: #12898a;
}
