<!--pages/article/article.wxml-->
<import src="../../wxParse/wxParse.wxml" />
<view class="page">
  <view class="header">
    <view class="i-back icon" bindtap="goBack">
      <image src="{{img}}i-back.png"></image>
    </view>
    <text>{{title}}</text>
    <view class="i-search icon" bindtap="goSearch">
        <image src="../../images/home.png"></image>
    </view>
  </view>
  <view class="content">
    <view class="title">
      <!-- {{subTitle}} -->
      <template is="wxParse" data="{{wxParseData:title1.nodes}}" /> 
    </view>
    <view class="detail">
      <template is="wxParse" data="{{wxParseData:article.nodes}}" />
    </view>
  </view>
</view>